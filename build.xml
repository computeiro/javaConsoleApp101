<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="jar" name="Create Runnable Jar for Project AlgoritmoEDII with libraries in sub-folder">
    <property name="jar.file" value="${basedir}/arvore-ctt.jar"/>
	
	<target name="jar">
        <jar destfile="${jar.file}">
            <manifest>
                <attribute name="Main-Class" value="agenda.Aplicacao"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="${basedir}/bin"/>
        </jar>
    </target>
</project>
